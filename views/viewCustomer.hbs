<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">Customer Profile</li>
  </ol>
</nav>

<div class="customer-profile p5">


    {{#each rows}}
        <div class="row mb-6">
            <div class="col text-center">
                <h2>{{this.first_name}} {{this.last_name}}</h2>
            </div>
        </div>

        <div class="row">
            <div class="col">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">First Name</th>
                            <th scope="col">Last Name</th>
                            <th scope="col">Email</th>
                            <th scope="col">Phone Number</th>
                            <th scope="col">Job Title</th>
                            <th scope="col">Industry</th>
                            <th scope="col">Website</th>
                            <th scope="col">Instagram</th>
                            <th scope="col">LinkedIn</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <th scope="row">{{this.first_name}}</th>
                            <td>{{this.last_name}}</td>
                            <td><a href="mailto:{{this.email}}">{{this.email}}</a></td>
                            <td>{{this.phone_number}}</td>
                            <td>{{this.job_title}}</td>
                            <td>{{this.industry}}</td>
                            <td><a href="//{{this.website}}" target="_blank">{{this.website}}</a></td>
                            <td><a href="//www.instragram.com/{{this.instagram}}/?hl=en" target="_blank">{{this.instagram}}</a></td>
                            <td>{{this.linkedin}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <div class="row">
                <div class="col">
                    <b>Customer Profile Notes</b>
                </div>
            </div>
            <div class="row">
                <div class="col">
                    {{this.notes}}
                </div>
            </div>
        </div>



    {{/each}}

</div>